    "\n",
    "while True:\n",
    "    led.value(1)  # Turn LED on\n",
    "    time.sleep(1)  # Wait for 1 second\n",
    "    led.value(0)  # Turn LED off\n",
    "    time.sleep(1)  # Wait for 1 second"
